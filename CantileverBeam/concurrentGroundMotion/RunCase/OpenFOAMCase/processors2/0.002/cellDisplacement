FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       decomposedBlockData;
    object      cellDisplacement;
    data.format     ascii;
    data.class      volVectorField;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


// processor0
1055
(FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0.002";
    object      cellDisplacement;
}
dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    leftWall
    {
        type            fixedNormalSlip;
        fixedValue      uniform (0 0 0);
    }
    rightWall
    {
        type            fixedNormalSlip;
        fixedValue      nonuniform List<vector> 0();
    }
    interface
    {
        type            cellMotion;
        value           nonuniform List<vector> 0();
    }
    atmosphere
    {
        type            fixedNormalSlip;
        fixedValue      uniform (0 0 0);
    }
    lowerWall
    {
        type            fixedNormalSlip;
        fixedValue      uniform (0 0 0);
    }
    defaultFaces
    {
        type            fixedNormalSlip;
        fixedValue      uniform (0 0 0);
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
}
)

